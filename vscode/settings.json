{
    "breadcrumbs.enabled": true,
    "editor.fontSize": 11,
    "editor.insertSpaces": false,
    "editor.showFoldingControls": "always",
    "editor.renderWhitespace": "boundary",
    "editor.minimap.enabled": true,
    "editor.rulers": [80],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "workbench.activityBar.visible": false,
    "terminal.integrated.fontSize": 12,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.colorTheme": "Monokai",
    "window.zoomLevel": 0,
    "python.pythonPath": "/usr/bin/python3",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style","/home/maralc/.config/yapf/yapf.ini"
    ],
    "python.formatting.autopep8Args": [
        "--ignore","W191,E111,E101"
    ],
    "python.jediEnabled": false,
    "python.unitTest.unittestEnabled": true,
    "vim.textwidth": 80,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block-outline",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.leader": ",",
    "vim.foldfix": true,
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>","t"],
            "after": [],
            "commands":[
                {
                    "command": ":tabnext",
                    "args": []
                }
            ]
        },
        {
            "before": ["<leader>","r"],
            "after": [],
            "commands":[
                {
                    "command": ":tabprevious",
                    "args": []
                }
            ]
        },
        {
            "before": ["<space>"],
            "after": ["<C-f>"]
        },
        {
            "before": ["<C-l>"],
            "after": ["<C-w>","l"]
        },
        {
            "before": ["<C-h>"],
            "after": ["<C-w>","h"]
        },
        {
            "before": ["<shift-space>"],
            "after": ["C-b>"]
        },
        {
            "before": ["<alt-j>"],
            "after": ["<C-d>"]
        },
        {
            "before": ["<alt-k>"],
            "after": ["<C-u>"]
        },
        {
            "before": ["<alt-i>"],
            "after": ["^"]
        },
        {
            "before": [",","y"],
            "after": ["\"*y"]
        },
        {
            "before": [",","p"],
            "after": ["\"*p"]
        },
        {
            "before": [",","P"],
            "after": ["\"*P"]
        },
        {
            "before": ["<leader>","S"],
            "after": [],
            "commands":[
                {
                    "command": "editor.action.quickFix",
                    "args": []
                }
            ]
        },
        {
            "before": ["]","s"],
            "after": [],
            "commands":[
                {
                    "command": "editor.action.marker.next",
                    "args": []
                }
            ]
        },
        {
            "before": ["[","s"],
            "after": [],
            "commands":[
                {
                    "command": "editor.action.marker.prev",
                    "args": []
                }
            ]
        }
    ],
    "cSpell.language": "en-GB",
    "cSpell.dictionaryDefinitions": [
        {
            "name": "custom-misc",
            "path": "/home/maralc/.vim/spell/custom-misc.utf-8.add"
        },
        {
            "name": "custom-python",
            "path": "/home/maralc/.vim/spell/custom-python.utf-8.add"
        }
    ],
    "cSpell.dictionaries": [
        "custom-misc"
    ],
    "cSpell.languageSettings": [
        {
            "languageId": "python",
            "dictionaries": ["custom-python"]
        }
    ],
    "restructuredtext.linter.run": "onSave",
    "restructuredtext.linter.extraArgs": [
        "--ignore","D001"
    ],
    "restructuredtext.confPath": "${workspaceFolder}/docs",
    "editor.minimap.renderCharacters": false,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.showStatus": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "window.menuBarVisibility": "toggle",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "todo-tree.general.tags":[
        "TODO",
        "REFACTOR",
        "ENHANCEMENT"
    ],
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^)*\\s*($TAGS)|^\\s*- \\[ \\])",
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "iconColour": "#ff0000",
            "rulerColour": "#ff0000",
            "background": "#ff0000",
            "foreground": "white",
            "opacity": 0.4,
            "type": "text"
        },
        "REFACTOR": {
            "iconColour": "#ffff00",
            "rulerColour": "#ffff00",
            "background": "#ffff00",
            "foreground": "white",
            "opacity": 0.4,
            "type": "text"
        },
        "ENHANCEMENT": {
            "iconColour": "#00ff00",
            "rulerColour": "#00ff00",
            "background": "#00ff00",
            "foreground": "white",
            "opacity": 0.4,
            "type": "text"
        }
    },
    "todo-tree.highlights.defaultHighlight": {
        "iconColour": "#ffffff",
        "rulerColour": "#ffffff",
        "background": "#ffffff",
        "foreground": "white",
        "opacity": 0.4,
        "type": "text"
    },
    "todo-tree.tree.grouped": true,
    "todo-tree.tree.showTagsFromOpenFilesOnly": true,
    "editor.fontFamily": "'Hasklig', monospace",
    "editor.fontLigatures": true,
}
